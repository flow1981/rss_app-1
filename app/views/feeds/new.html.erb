

<div>
  <h3>Add Feed</h3>
  <%= form_tag ({controller: 'dashboard', action: 'new_folder_or_feed', method: 'post'}) do %>
    <label>Feed URL: </label>
    <%= text_field(:feed, :link) %><br>
    <label>Folder Name: </label>
    <%= text_field(:folder, :name) %><br>
    <%= collection_select(:feed, :folder_id, @user.folders, :id, :name) %>
    <input type=submit value='add feed'>
  <% end %>
</div>