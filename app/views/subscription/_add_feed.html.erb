
<%= form_for @feed, url: '/create_feed', method: :post do |f|%>
        
    <div class="form-group">

        <%= f.label :link, "Feed URL"%>
        <%= f.text_field :link, class: "form-control", placeholder: "Feed URL" %>
    </div> 

    <div class="form-group">

        <%= f.label :feed, "Folder Name"%>
        <%= f.collection_select :feed, :folder_id, @user.folders, :id, :name %>


    </div> 
        
    <input type=submit value='add feed'>

<% end %>

