<h1> Sign Up Page </h1>
  
<%= form_for @user, url: '/signup', method: :post do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  
  <%= f.label :username %>
  <%= f.text_field :username %><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %><br>

  <%= f.submit "Submit" %>
<% end %>
 